{
  "version": 2,
  "functions": {
    "api/**/*.ts": {
      "runtime": "nodejs20.x",
      "includeFiles": "prisma/dev.db"
    },
    "app/api/**/*.ts": {
      "runtime": "nodejs20.x",
      "includeFiles": "prisma/dev.db"
    }
  },
  "installCommand": "npm install && npx prisma generate",
  "buildCommand": "npm run build",
  "env": {
    "DATABASE_URL": "file:./prisma/dev.db"
  },
  "build": {
    "env": {
      "DATABASE_URL": "file:./prisma/dev.db",
      "NEXT_PRIVATE_TURBOPACK": "0",
      "TURBOPACK": "0",
      "NEXT_MANUAL_SIGSTORE": "1"
    }
  }
}